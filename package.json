{
  "name": "PiBakery",
  "displayName": "PiBakery",
  "version": "0.2.3",
  "description": "The blocks based, easy to use setup tool for Raspberry Pi",
  "main": "./main.js",
  "author": "David Ferguson <david@pibakery.org>",
  "license": "GPL-3.0+",
  "scripts": {
    "start": "electron .",
    "build-mac": "find . -name .DS_Store -depth -exec rm {} \\; && npm dedupe && electron-packager ./ PiBakery --platform=darwin --arch=all --version=1.3.2 --overwrite --prune --app-version=0.2.3 --icon=./app/img/icon.icns",
    "build-win": "npm dedupe && electron-packager ./ PiBakery --platform=win32 --arch=ia32 --version=1.3.2 --overwrite --prune --app-version=0.2.3 --icon=./app/img/icon.ico"
  },
  "optionalDependencies": {
    "elevator": "^1.0.0",
    "removedrive": "^1.1.1"
  },
  "dependencies": {
    "chmod": "^0.2.1",
    "drivelist": "^3.2.6",
    "drivelist-scanner": "^1.0.0",
    "electron-localshortcut": "^0.6.1",
    "extract-zip": "^1.5.0",
    "fs-extra": "^0.30.0",
    "is-elevated": "^1.0.0",
    "is-online": "^5.1.2",
    "md5-file": "^3.1.1",
    "node-df": "https://github.com/beeman/node-df/tarball/master",
    "node-shell-parser": "^0.2.1",
    "request": "^2.74.0",
    "resin-image-write": "^4.0.2",
    "sudo-prompt": "^6.1.0",
    "umount": "^1.1.3",
    "wget-improved": "^1.3.0"
  },
  "devDependencies": {
    "electron-packager": "^7.4.0",
    "electron-prebuilt": "^1.3.2"
  }
}
